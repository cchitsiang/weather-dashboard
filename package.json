{
  "name": "weather-dashboard",
  "version": "1.0.0",
  "description": "A simple weather dashboard come with responsive",
  "main": "./dist/server.js",
  "scripts": {
    "build": "NODE_ENV=production webpack",
    "build-babel": "npm run build-babel-server && npm run build-babel-client",
